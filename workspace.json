            "outputPath": "dist/apps/traqrcode",
            "index": "apps/traqrcode/src/index.html",
            "main": "apps/traqrcode/src/main.tsx",
            "polyfills": "apps/traqrcode/src/polyfills.ts",
            "tsConfig": "apps/traqrcode/tsconfig.app.json",
            "assets": [
              "apps/traqrcode/src/favicon.ico",
              "apps/traqrcode/src/assets"
            ],
            "styles": ["apps/traqrcode/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
