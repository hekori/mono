            "jestConfig": "libs/cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mail": {
      "root": "libs/mail",
      "sourceRoot": "libs/mail/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
