{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "projects": {
    "uikit": {
      "root": "libs/uikit",
      "sourceRoot": "libs/uikit/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/uikit/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/uikit"],
          "options": {
            "jestConfig": "libs/uikit/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "cli": {
      "root": "libs/cli",
      "sourceRoot": "libs/cli/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/cli/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/cli"],
          "options": {
            "jestConfig": "libs/cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mail": {
      "root": "libs/mail",
      "sourceRoot": "libs/mail/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/mail/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/mail"],
          "options": {
            "jestConfig": "libs/mail/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "traqrcode-ui": {
      "root": "projects/traqrcode/traqrcode-ui",
      "sourceRoot": "projects/traqrcode/traqrcode-ui/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/traqrcode/traqrcode-ui",
            "index": "projects/traqrcode/traqrcode-ui/src/index.html",
            "main": "projects/traqrcode/traqrcode-ui/src/main.tsx",
            "polyfills": "projects/traqrcode/traqrcode-ui/src/polyfills.ts",
            "tsConfig": "projects/traqrcode/traqrcode-ui/tsconfig.app.json",
            "assets": [
              "projects/traqrcode/traqrcode-ui/src/assets"
            ],
            "styles": ["projects/traqrcode/traqrcode-ui/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/traqrcode/traqrcode-ui/src/environments/environment.ts",
                  "with": "projects/traqrcode/traqrcode-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "traqrcode-ui:build",
            "hmr": true,
            "port": 3002
          },
          "configurations": {
            "production": {
              "buildTarget": "traqrcode-ui:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/traqrcode/traqrcode-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/traqrcode/traqrcode-ui"],
          "options": {
            "jestConfig": "projects/traqrcode/traqrcode-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "traqrcode-e2e": {
      "root": "projects/traqrcode/traqrcode-e2e",
      "sourceRoot": "projects/traqrcode/traqrcode-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "projects/traqrcode/traqrcode-e2e/cypress.json",
            "tsConfig": "projects/traqrcode/traqrcode-e2e/tsconfig.e2e.json",
            "devServerTarget": "traqrcode:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "traqrcode:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/traqrcode/traqrcode-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "traqrcode-api": {
      "root": "projects/traqrcode/traqrcode-api",
      "sourceRoot": "projects/traqrcode/traqrcode-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/projects/traqrcode/traqrcode-api",
            "main": "projects/traqrcode/traqrcode-api/src/main.ts",
            "tsConfig": "projects/traqrcode/traqrcode-api/tsconfig.app.json",
            "assets": ["projects/traqrcode/traqrcode-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "projects/traqrcode/traqrcode-api/src/environments/environment.ts",
                  "with": "projects/traqrcode/traqrcode-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "traqrcode-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/traqrcode/traqrcode-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/projects/traqrcode/traqrcode-api"],
          "options": {
            "jestConfig": "projects/traqrcode/traqrcode-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "traqrcode-common": {
      "root": "projects/traqrcode/traqrcode-common",
      "sourceRoot": "projects/traqrcode/traqrcode-common/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["projects/traqrcode/traqrcode-common/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/traqrcode-common"],
          "options": {
            "jestConfig": "projects/traqrcode/traqrcode-common/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dates": {
      "root": "libs/dates",
      "sourceRoot": "libs/dates/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/dates/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dates"],
          "options": {
            "jestConfig": "libs/dates/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "uibook": {
      "root": "apps/uibook",
      "sourceRoot": "apps/uibook/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/uibook",
            "index": "apps/uibook/src/index.html",
            "main": "apps/uibook/src/main.tsx",
            "polyfills": "apps/uibook/src/polyfills.ts",
            "tsConfig": "apps/uibook/tsconfig.app.json",
            "assets": ["apps/uibook/src/favicon.ico", "apps/uibook/src/assets"],
            "styles": ["apps/uibook/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/uibook/src/environments/environment.ts",
                  "with": "apps/uibook/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "uibook:build",
            "hmr": true,
            "port": 5001
          },
          "configurations": {
            "production": {
              "buildTarget": "uibook:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/uibook/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/uibook"],
          "options": {
            "jestConfig": "apps/uibook/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "knexutils": {
      "root": "libs/knexutils",
      "sourceRoot": "libs/knexutils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/knexutils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/knexutils"],
          "options": {
            "jestConfig": "libs/knexutils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
