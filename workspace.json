          "options": {
            "jestConfig": "apps/traqrcode/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "traqrcode-e2e": {
      "root": "apps/traqrcode-e2e",
      "sourceRoot": "apps/traqrcode-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
