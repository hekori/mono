    "clean": "rimraf ./dist",
    "postcss": "postcss libs/uikit/src/lib/tailwind.css -o libs/uikit/src/lib/App.css",
    "traqrcode:pg:destroy": "dotenv -e ../.env -- docker-compose --file projects/traqrcode/traqrcode-docker/docker-compose.postgres.yml down --volumes",
    "traqrcode:pg:start": "dotenv -e ../.env -- docker-compose --file projects/traqrcode/traqrcode-docker/docker-compose.postgres.yml up -d postgres",
    "traqrcode:pg:stop": "dotenv -e ../.env -- docker-compose --file projects/traqrcode/traqrcode-docker/docker-compose.postgres.yml stop --timeout 1 -- postgres   ",
    "traqrcode:psql": "docker-compose --file projects/traqrcode/traqrcode-backend/docker-compose.yml exec postgres pqsl",
    "traqrcode:cli": "dotenv -e ../.env -- ts-node --project projects/traqrcode/traqrcode-backend/tsconfig.app.json -r tsconfig-paths/register projects/traqrcode/traqrcode-backend/cli.ts",
    "traqrcode:build:prod:backend": "dotenv -e ../.env -- nx run traqrcode-backend:build --prod",
    "traqrcode:build:prod:frontend": "dotenv -e ../.env -- nx run traqrcode-frontend:build --prod",
    "traqrcode:local-serve-build:frontend": "npx spa-server-cli -p 3003 dist/projects/traqrcode/traqrcode-frontend",
    "traqrcode:provision": "yarn fly -f projects/traqrcode/traqrcode-deploy/flightplan.ts provision:prod",
    "traqrcode:deploy": "yarn fly -f projects/traqrcode/traqrcode-deploy/flightplan.ts deploy:prod",
    "traqrcode:deploy:backend": "dotenv -e ../.env -- ts-node --project projects/traqrcode/traqrcode-backend/tsconfig.app.json -r tsconfig-paths/register projects/traqrcode/traqrcode-backend/cli.ts",
    "traqrcode:serve:frontend": "dotenv -e ../.env -- nx run traqrcode-frontend:serve",
    "traqrcode:serve:backend": "dotenv -e ../.env -- nx run traqrcode-backend:serve",
    "traqrcode:depcruise": "depcruise --exclude \".*\\.spec\\..*\" --include-only \"src\" --output-type dot projects/traqrcode | dot -T svg > dependencygraph.svg  "
  },
